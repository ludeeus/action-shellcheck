{
    "problemMatcher": [
      {
        "owner": "shellcheck-tty",
        "pattern": [
          {
              "regexp": "^In\\s\\.?\\/?(.+)\\sline\\s(\\d+):$",
              "file": 1,
              "line": 2
          },
           {
              "regexp": "^.*(SC\\d+)\\s\\((\\w+)\\):\\s(.+)$",
              "code": 1,
              "severity": 2,
              "message": 3,
              "loop": true
          }
        ]
      }
    ]
  }
